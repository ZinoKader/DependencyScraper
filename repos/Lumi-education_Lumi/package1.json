{
    "name": "lumi",
    "version": "v0.8.0",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/Lumi"
    },
    "scripts": {
        "start": "npm run build && npm run dev",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "jest",
        "test:update": "jest -u",
        "test:integration": "./test/integration/integration.test.js",
        "clean": "rm -rf build && rm -rf dist && rm -rf app",
        "uninstall": "npm run clean && rm -rf node_modules",
        "dev": "./node_modules/.bin/concurrently 'npm run dev:server' 'npm run dev:client'",
        "dev:server": "npm run build:server && node ./build/server/boot.js",
        "dev:client": "NODE_ENV=development VERSION=$(git describe --tags) ./node_modules/.bin/webpack-dev-server --config './config/webpack.config.dev.js' --progress --colors --hot --inline",
        "build": "npm run build:client && npm run build:server && cp electron/main.js build/",
        "build:server": "npm run lint:server  && ./node_modules/.bin/tsc --project server/tsconfig.server.json &&  cp -r server/views build/server && cp server/db/driver/pouchdb.config.json build/server/db/driver",
        "build:client": "NODE_ENV=production VERSION=$(echo ${VERSION}) TAG=$(echo ${TAG}) ./node_modules/.bin/webpack --config './config/webpack.config.prod.js' --progress --colors",
        "lint:client": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
        "lint:server": "./node_modules/.bin/tslint --project server/tsconfig.server.json --config tslint.json",
        "build:mac": "npm run clean && npm run build && cp electron/main.js build/ && ./node_modules/.bin/build install-app-dep &&./node_modules/.bin/build -m",
        "publish": "npm run clean && npm run build && cp electron/main.js build/ && ./node_modules/.bin/build install-app-dep &&./node_modules/.bin/build --mac -p always",
        "postinstall": "ln -s ../lib node_modules/lib && ln -s ../client node_modules/client && ln -s ../server node_modules/server"
    },
    "author": "Jan Philip Schellenberg",
    "dependencies": {
        "@material-ui/core": "^3.6.0",
        "@material-ui/lab": "^3.0.0-alpha.26",
        "@types/express": "4.0.37",
        "@types/express-serve-static-core": "4.0.49",
        "@types/lodash": "4.14.119",
        "@types/raven": "^2.1.4",
        "@types/react": "16.4.18",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "copy-dir": "^0.4.0",
        "cp-file": "^6.0.0",
        "debug": "2.3.3",
        "electron-updater": "^4.0.6",
        "envfile": "^2.3.0",
        "express": "^4.16.0",
        "express-fileupload": "^0.4.0",
        "express-http-proxy": "^1.5.0",
        "express-pouchdb": "^4.1.0",
        "express-react-views": "^0.10.5",
        "h5p-nodejs-library": "0.0.17",
        "jwt-simple": "^0.5.1",
        "lodash": "^4.17.11",
        "mkdirp": "^0.5.1",
        "mv": "^2.1.1",
        "nano": "^7.1.1",
        "native-dns": "^0.7.0",
        "node-machine-id": "^1.1.10",
        "particles.js": "^2.0.0",
        "pouchdb": "^7.0.0",
        "pouchdb-adapter-node-websql": "^7.0.0",
        "pouchdb-find": "^7.0.0",
        "query-string": "^5.0.1",
        "raven": "^2.4.0",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "react-particles-js": "^2.4.2",
        "reactour": "^1.13.0",
        "recursive-readdir": "^2.2.2",
        "rimraf": "^2.6.2",
        "semver": "^5.5.1",
        "styled-components": "^4.1.3",
        "sudo-prompt": "^8.2.5",
        "superagent": "^3.8.3",
        "supertest": "^3.3.0",
        "tslint": "^5.7.0",
        "tslint-config-airbnb": "^5.3.1",
        "tslint-config-prettier": "^1.6.0",
        "tslint-loader": "^3.5.3",
        "tslint-react": "3.2.0",
        "typescript": "^3.1.3",
        "unzip": "^0.1.11",
        "url-parse": "^1.4.3",
        "url-to-screenshot": "^3.0.0"
    },
    "devDependencies": {
        "@material-ui/icons": "^3.0.1",
        "@types/classnames": "0.0.32",
        "@types/material-ui": "0.20.1",
        "@types/node": "^7.0.2",
        "@types/raven-js": "^3.10.0",
        "@types/react-dom": "0.14.20",
        "@types/react-redux": "4.4.35",
        "@types/socket.io": "^1.4.31",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "chart.js": "^2.7.2",
        "classnames": "2.2.5",
        "concurrently": "^4.0.1",
        "connected-react-router": "^4.5.0",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "debug": "^2.3.3",
        "electron": "^3.0.6",
        "electron-builder": "^20.29.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-15.4": "^1.1.1",
        "enzyme-to-json": "^3.2.2",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.2.1",
        "jest-cli": "^21.2.1",
        "markdown-it": "8.4.0",
        "markdown-it-latex": "^0.2.0",
        "material-ui": "0.20.1",
        "material-ui-color-picker": "^2.1.1",
        "moment": "^2.20.1",
        "moment-timezone": "^0.5.14",
        "newman": "^4.2.2",
        "raven-for-redux": "^1.2.0",
        "raven-js": "^3.22.1",
        "react-addons-test-utils": "^15.6.2",
        "react-avatar-editor": "^11.0.4",
        "react-beautiful-dnd": "^10.0.2",
        "react-chartjs-2": "^2.7.2",
        "react-chartkick": "^0.3.0",
        "react-dropzone": "^4.2.3",
        "react-hot-loader": "1.3.0",
        "react-infinite-scroller": "^1.2.2",
        "react-katex": "^2.0.2",
        "react-redux": "5.0.7",
        "react-redux-i18n": "^1.9.3",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "4.0.8",
        "react-select": "^2.1.1",
        "react-sortable-hoc": "0.6.7",
        "redux": "4.0.1",
        "redux-thunk": "2.1.0",
        "shortid": "^2.2.14",
        "socket.io-client": "^2.0.4",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.23.0",
        "ts-jest": "^21.2.3",
        "ts-loader": "2.3.7",
        "typeface-roboto": "0.0.54",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "webpack": "^3.8.1",
        "webpack-dev-server": "2.9.4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(server|client/*/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "moduleDirectories": [
            "node_modules",
            "client",
            "lib",
            "server"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "main": "build/main.js",
    "build": {
        "appId": "org.lumi.education",
        "productName": "Lumi",
        "files": [
            "build/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "mac": {
            "category": "org.lumi.education",
            "publish": "github",
            "fileAssociations": {
                "ext": "h5p",
                "name": "H5P"
            }
        },
        "win": {
            "publish": "github"
        }
    }
}
